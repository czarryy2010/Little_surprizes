<h2> New wish list </h2>

<% form_for :wish_list, :url => wish_lists_path,:html => { :multipart => true }  do |f| %>
  <%= f.error_messages %>
<div class="form">
    <table cellpadding="3px">
		    <tr>
  				<td> <label>Wishlist Name: </label> </td><td><%= f.text_field :name %></td>
        </tr>
        <tr>
          <td> <label>Wishlist Description: </label> </td><td><%= f.text_field :description %></td>
        </tr>
    </table>

   <p>
    <%= f.submit 'Create' %> -or- <%= link_to 'Back', wish_lists_path %>
  </p>
 </div>
<% end %>

