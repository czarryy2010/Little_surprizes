
 <table class="admin_listing">
  <tr>
     <th> Name </th>
     <th> Points earned </th>
     <th> Point Details </th>
     <th> Deliver Gift </th>
  </tr>

  <% @users.each do |user| -%>
   <tr class="<%= cycle('odd', 'even')%>">
        <td> <%= user.name %> </td>
        <td> <%= "$ #{user.points}" %> </td>
        <td> <%= link_to "Details",  admin_user_points_path(user.id)  %> </td>
        <td> <%= link_to "Giveout gifts",  admin_user_gifts_path(user.id)  %>  </td>
    </tr>

  <% end -%>

