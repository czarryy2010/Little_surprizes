<div id="wish_list">
 <h2><%= @wish_list.name %></h2>
 <hr />
 <p><%= @wish_list.description%></p>
<table class="admin_listing">
   <tr>
	<th>Category name</th>
	<th>Comment</th>
   </tr>
 <% @wish_list_categories.each do |wish_catogery|%>

    <tr class="<%= cycle('odd', 'even')%>">
	<td><%= wish_catogery.category.name %></td>
	<td><%= wish_catogery.custom_description %></td>
   </tr>
 <% end %>
</table>
</div>
</div>

